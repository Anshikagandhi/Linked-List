class node{
public:
    int data;
    node * next;
    node(int data){
        this->data=data;
        this->next=NULL;
    }
};

voiprint_linkedlist_spl(node*head)
{
    //write your code here
 
  if(head->next!=NULL)
    return head;
  node *temp=print_linkedlist_spl(head->next);
  while(temp->next!=NULL)
  {
    temp=temp->next;
  }
  temp->next=head;
  head=temp;
  return head;
}
